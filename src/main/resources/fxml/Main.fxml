<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="601.0" prefWidth="875.0" stylesheets="@../css/Main.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.hbuas.netdisk.control.MainControl">
   <children>
      <HBox alignment="CENTER_LEFT" layoutY="-2.0" prefHeight="76.0" prefWidth="923.0" spacing="24.0" style="-fx-background-color: #eef0f6;">
         <children>
            <ImageView fitHeight="50.0" fitWidth="135.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/logo2.png" />
               </image>
            </ImageView>
            <Button fx:id="myNetDiskBtn" mnemonicParsing="false" onAction="#myNetDisk" prefHeight="34.0" prefWidth="99.0" style="-fx-background-color: #eef0f6;" text="我的网盘">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="transferBtn" mnemonicParsing="false" onAction="#transferList" prefHeight="34.0" prefWidth="94.0" style="-fx-background-color: #eef0f6;" text="传输列表">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="uploadBtn" mnemonicParsing="false" onAction="#uploadFile" prefHeight="34.0" prefWidth="92.0" style="-fx-background-color: #eef0f6;" text="上传文件">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="newBtn" mnemonicParsing="false" prefHeight="34.0" prefWidth="112.0" style="-fx-background-color: #eef0f6;" text="新建文件夹">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <TextField fx:id="searchText" onAction="#searchFile2" prefHeight="30.0" prefWidth="170.0" promptText="搜索我的网盘文件" style="-fx-background-color: #eef0f6;" />
            <Button mnemonicParsing="false" onAction="#searchFile1" prefHeight="34.0" prefWidth="71.0" style="-fx-background-color: #eef0f6;" text="搜索">
               <font>
                  <Font size="16.0" />
               </font></Button>
         </children>
      </HBox>
      <Pane layoutY="74.0" prefHeight="562.0" prefWidth="135.0" style="-fx-background-color: #f9fafb;">
         <children>
            <VBox alignment="TOP_CENTER" layoutY="28.0" prefHeight="343.0" prefWidth="135.0" spacing="20.0" style="-fx-background-color: #f9fafb;">
               <children>
                  <ImageView fx:id="images" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/logo.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="username" text="用户名">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </Pane>
      <ScrollPane fx:id="scrollPane" layoutX="135.0" layoutY="75.0" prefHeight="556.0" prefWidth="786.0" style="-fx-background-color: white;">
        <content>
          <AnchorPane fx:id="scrollContent" minHeight="535.0" minWidth="786.0" style="-fx-background-color: white;">
          </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
